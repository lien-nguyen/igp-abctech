[docker-hosts]
docker-server ansible_host=<DOCKER_HOST_IP> ansible_user=ansibleadmin ansible_ssh_private_key_file=~/.ssh/id_rsa

[k8s-masters]
k8s-master ansible_host=<K8S_MASTER_IP> ansible_user=ansibleadmin ansible_ssh_private_key_file=~/.ssh/id_rsa

[k8s-workers]
k8s-worker1 ansible_host=<K8S_WORKER1_IP> ansible_user=ansibleadmin ansible_ssh_private_key_file=~/.ssh/id_rsa
k8s-worker2 ansible_host=<K8S_WORKER2_IP> ansible_user=ansibleadmin ansible_ssh_private_key_file=~/.ssh/id_rsa

[jenkins-servers]
jenkins-server ansible_host=<JENKINS_HOST_IP> ansible_user=ansibleadmin ansible_ssh_private_key_file=~/.ssh/id_rsa

[all:vars]
ansible_python_interpreter=/usr/bin/python3
